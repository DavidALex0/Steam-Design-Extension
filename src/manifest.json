{
  "manifest_version": 2,
  "name": "Steam.Design Buttons",
  "version": "0.1.6",
  "description": "Creates Steam.Design buttons on steam.",
  "converted_from_user_script": true,

  "icons": {
    "48": "icon48.png",
   "128": "icon128.png" 
  },

  "content_scripts": [{
    "matches": ["http://www.steamcardexchange.net/*", "https://www.steamcardexchange.net/*", "https://steamcommunity.com/*", "http://steamcommunity.com/*"],
    "js": ["main.js"],
    "run_at": "document_end"

  }]
}
